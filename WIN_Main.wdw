     procedure_id : 1760449013805204002
     type_code : 14
     code : |1-
      PROCEDURE OpenFile(LOCAL sNameTXTFile is string)
      
      // Only take the .txt files into account
      IF fExtractPath(sNameTXTFile, fExtension) ~= ".TXT" THEN 
      	
      	// Open a text file received in parameter in a viewing MDI child window
      	MDIOpen(WIN_Document, "", sNameTXTFile, sNameTXTFile)
      	
      	// Pass the file in the last opened documents
      	gclLastDocuments:PassInFront(sNameTXTFile)
      	
      END
     type : 458752
