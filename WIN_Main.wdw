             sNameTXTFile		is string
             
             // Select a text file to view
             sNameTXTFile = fSelect("", "", <§$0000§>, <§$0001§>+TAB+"*.*"+CR+<§$0003§>+TAB+"*.TXT", "TXT", fselOpen+fselExist)
             
             // Was a file selected?
             IF sNameTXTFile <> "" THEN 
             	
             	IF fExtractPath(sNameTXTFile, fExtension) ~= ".txt" THEN
             		OpenFile(sNameTXTFile)
             	ELSE
             		// Inform the user that no text file was opened
             		Info("The selected file is not a text file.")
